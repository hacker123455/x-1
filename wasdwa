if game.Players.LocalPlayer.Name == "iRealKardes345" or game.Players.LocalPlayer.Name == "erdemhackeroyunda8" or game.Players.LocalPlayer.Name == "BarisOyunda01" or game.Players.LocalPlayer.Name == "SenseiPanky" or game.Players.LocalPlayer.Name == "superplayerXYZ_2" then -- Whitelisted <-----------
else
game.Players.LocalPlayer:Kick("Sanırım Hala Satın Almamışsın /:  Satın Almak İçin Ice & Fire#0001 e Mesaj Atmalısın") -- Kicks anyone without the name "ROBLOX"
end

while true do
wait(1.5)
for index, child in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
if child.ClassName == "Hat" or child.ClassName == "Accessory" then
if child.Handle:FindFirstChild("Mesh") then
child.Handle.Mesh.Parent = nil
end
child.Parent = game.Workspace
end
end
wait(0.9)
game.Workspace.erdemhackeroyunda8.Head:Destroy()
wait(5)
end
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextButton = Instance.new("TextButton")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(94, 94, 94)
Frame.BorderColor3 = Color3.fromRGB(94, 94, 94)
Frame.Position = UDim2.new(-2.98023224e-08, 0, 0.953316927, 0)
Frame.Size = UDim2.new(0, 55, 0, 38)

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(81, 81, 81)
TextButton.BorderColor3 = Color3.fromRGB(81, 81, 81)
TextButton.Position = UDim2.new(0.127272725, 0, 0.157894731, 0)
TextButton.Size = UDim2.new(0, 42, 0, 26)
TextButton.Font = Enum.Font.SciFi
TextButton.Text = "Rejoin"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextSize = 14.000

-- Scripts:

local function AKVNEMH_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
end)
	
end
coroutine.wrap(AKVNEMH_fake_script)()
